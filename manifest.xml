<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!--
https://learn.microsoft.com/en-us/office/dev/add-ins/develop/xml-manifest-overview?tabs=tabid-1
Must Contain:
  Id
  Version
  ProviderName
  DefaultLocale
  DefaultSettings
  DisplayName
  Description
  SupportUrl (if distributed through AppSource)
  DefaultSettings
  SourceLocation
  Permissions
  Set\s
  Hosts

Must access content hosted on https
-->
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type= "TaskPaneApp">
  <!-- GUID format -->
  <Id>17a6e5a4-0874-451f-a17a-8b2a53e422d3</Id>
  <!-- Can have multiple versions -->
  <Version>1.0.0.0</Version>

  <ProviderName>Olivers Addin</ProviderName>
  <!-- BCP 47 language tag format -->
  <DefaultLocale>en-AU</DefaultLocale>

  <DisplayName DefaultValue="MyExcelTest"/>

  <Description DefaultValue="This is the description."/>
  
  <DefaultSettings>
    <SourceLocation DefaultValue="https://www.wikipedia.org/" />
  </DefaultSettings>

  <!-- [Restricted | ReadDocument | ReadAllDocument | WriteDocument | ReadWriteDocument] -->
  <Permissions>ReadWriteDocument</Permissions>

  <Requirements>
    <Sets DefaultMinVersion="1.1">
      <Set Name="SharedRuntime" MinVersion="1.1"/>
    </Sets>
  </Requirements>

  <Hosts>
    <Host Name="Workbook"/>
  </Hosts>


<!-- Not Rrequired: Taskpane -->
  <VersionOverrides xmlns="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="VersionOverridesV1_0">
    <Hosts>
    <!-- Can have different settings for multiple hosts. -->
      <Host xsi:type="Workbook">
          <!-- Modifying the ribbon -->
          <ExtensionPoint xsi:type="PrimaryCommandSurface">
            <CustomTab DefaultValue="Olivers New Tab">
              <Group DefaultValue="Dodgy Commands">
                <Label DefaultValue="Label"/>
